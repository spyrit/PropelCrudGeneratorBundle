{
    "name": "spyrit/propel-crud-generator-bundle",
    "type": "symfony-bundle",
    "description": "A generator for PHP code based on Twig template engine",
    "keywords": ["twig", "generator", "code generation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre-Yves Guerder",
            "email": "pierre-yves.guerder@spyrit.net"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "twig/twig": ">=1.1, <2.0-dev",
        "propel/propel": "2.0.x-dev",
        "propel/propel-bundle": "3.0.x-dev"
    },
    "autoload": {
        "psr-4": { "Spyrit\\PropelCrudGeneratorBundle\\": "" },
        "exclude-from-classmap": [ "Resources/" ]
    },
    "target-dir": "Spyrit/PropelCrudGeneratorBundle"
}
